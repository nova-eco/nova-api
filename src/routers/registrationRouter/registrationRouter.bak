import { routerOptions } from '@app/config';
import { Router } from 'express';
import { registrationRouterGetRegistrationVerified } from './registrationRouterGetRegistrationVerified.bak';
import { registrationRouterPostRegistration } from './registrationRouterPostRegistration';
import { registrationRouterPostRegistrationMessage } from './registrationRouterPostRegistrationMessage';

export const registrationRouter = Router(routerOptions)
  .use('/', registrationRouterPostRegistration)
  .use('/:registrationId/message', registrationRouterPostRegistrationMessage)
  .use('/:registrationId/verify/:accessCode', registrationRouterGetRegistrationVerified);
