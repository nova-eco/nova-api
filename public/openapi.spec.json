  "info": {
    "title": "Nova API",
    "version": "2.0.0",
    "description": "Nova Backend API - A booking management system for workplace/salon bookings with support for services, staff, and appointments"
  },
  "openapi": "3.0.4",
  "paths": {
    "/v2/booking/pre": {
      "post": {
        "summary": "Create a pre-booking",
        "description": "Creates a tentative booking reservation that must be confirmed later",
        "tags": ["Booking"],
        "requestBody": {
          "required": true,
          "content": {
            "application/json": {
              "schema": {
                "$ref": "#/components/schemas/BookingPreRequest"
              }
            }
          }
        },
        "responses": {
          "201": {
            "description": "Pre-booking created successfully",
            "content": {
              "application/json": {
                "schema": {
                  "$ref": "#/components/schemas/BookingPre"
                }
              }
            }
          },
          "400": {
            "$ref": "#/components/responses/BadRequest"
          },
          "500": {
            "$ref": "#/components/responses/InternalServerError"
          }
        }
      }
    },
    "/v2/booking/pre/{companyId}": {
      "get": {
        "summary": "Get pre-bookings by company",
        "description": "Retrieves all pre-bookings for a specific company",
        "tags": ["Booking"],
        "parameters": [
          {
            "name": "companyId",
            "in": "path",
            "required": true,
            "schema": {
              "type": "string"
            },
            "description": "The company ID"
          }
        ],
        "responses": {
          "200": {
            "description": "List of pre-bookings",
            "content": {
              "application/json": {
                "schema": {
                  "type": "array",
                  "items": {
                    "$ref": "#/components/schemas/BookingPre"
                  }
                }
              }
            }
          },
          "404": {
            "$ref": "#/components/responses/NotFound"
          },
          "500": {
            "$ref": "#/components/responses/InternalServerError"
          }
        }
      }
    },
    "/v2/booking": {
      "post": {
        "summary": "Create a booking from pre-booking",
        "description": "Converts a pre-booking into a confirmed booking with order and appointment",
        "tags": ["Booking"],
        "requestBody": {
          "required": true,
          "content": {
            "application/json": {
              "schema": {
                "$ref": "#/components/schemas/BookingRequest"
              }
            }
          }
        },
        "responses": {
          "201": {
            "description": "Booking created successfully",
            "content": {
              "application/json": {
                "schema": {
                  "$ref": "#/components/schemas/BookingResponse"
                }
              }
            }
          },
          "400": {
            "$ref": "#/components/responses/BadRequest"
          },
          "500": {
            "$ref": "#/components/responses/InternalServerError"
          }
        }
      }
    },
    "/v2/chair": {
      "post": {
        "summary": "Create a chair/seat",
        "description": "Creates a new chair (seat) and associates it with a salon and services",
        "tags": ["Chair"],
        "requestBody": {
          "required": true,
          "content": {
            "application/json": {
              "schema": {
                "$ref": "#/components/schemas/ChairRequest"
              }
            }
          }
        },
        "responses": {
          "201": {
            "description": "Chair created successfully",
            "content": {
              "application/json": {
                "schema": {
                  "$ref": "#/components/schemas/Chair"
                }
              }
            }
          },
          "400": {
            "$ref": "#/components/responses/BadRequest"
          },
          "500": {
            "$ref": "#/components/responses/InternalServerError"
          }
        }
      }
    },
    "/v2/chair/{seatId}": {
      "patch": {
        "summary": "Update a chair",
        "description": "Updates an existing chair's properties and service associations",
        "tags": ["Chair"],
        "parameters": [
          {
            "name": "seatId",
            "in": "path",
            "required": true,
            "schema": {
              "type": "string"
            },
            "description": "The seat/chair ID"
          }
        ],
        "requestBody": {
          "required": true,
          "content": {
            "application/json": {
              "schema": {
                "$ref": "#/components/schemas/ChairUpdateRequest"
              }
            }
          }
        },
        "responses": {
          "200": {
            "description": "Chair updated successfully",
            "content": {
              "application/json": {
                "schema": {
                  "$ref": "#/components/schemas/Chair"
                }
              }
            }
          },
          "404": {
            "$ref": "#/components/responses/NotFound"
          },
          "500": {
            "$ref": "#/components/responses/InternalServerError"
          }
        }
      }
    },
    "/v2/location": {
      "post": {
        "summary": "Create a location",
        "description": "Creates a new physical location with address details",
        "tags": ["Location"],
        "requestBody": {
          "required": true,
          "content": {
